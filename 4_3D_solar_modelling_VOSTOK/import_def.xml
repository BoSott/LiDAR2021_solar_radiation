<!-- OPALS Format Definition Section --> <opalsFormatDefinition>
<description>Import Format: xyz R G B</description>
<!-- Example for a simple XYZ ASCII Format Definition for Import and Export -->
<ascii>
    <header>
        <skipLines val="0" />
    </header>
    <commentInitiator val="#" />
    <data>
        <entry val="x" />
        <entry val="y" />
        <entry val="z" />
    </data>
</ascii>
</opalsFormatDefinition>